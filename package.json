{
  "name": "node-vimeo",
  "version": "1.0.0",
  "description": "Nodejs Framework for scrapping vimeo Videos with Readable Stream and its a Un-Official vimeo Package",
  "main": "./src/index.js",
  "module": "./src/index.mjs",
  "types": "./typings/index.d.ts",
  "files": [
    "src",
    "typings"
  ],
  "exports": {
    "./*": "./*",
    ".": {
      "require": "./src/index.js",
      "import": "./src/index.mjs"
    }
  },
  "scripts": {
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "lint:commit": "npm run lint:fix && npm run commit:push",
    "commit": "git-cz",
    "semantic-release": "semantic-release --branches main",
    "commit:push": "git add . && npm run commit && git push",
    "test": "node test/testing.js",
    "docs": "jsdoc node_modules/.bin/jsdoc --configure .jsdoc.json --verbose"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "jsdoc": "^3.6.10",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-skyceil": "^2.0.0",
    "nyc": "^15.1.0",
    "semantic-release": "^19.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">= 17.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "fs": "^0.0.1-security",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "path": "^0.12.7",
    "pretty-ms": "^7.0.1",
    "stream": "^0.0.2"
  },
  "directories": {
    "example": "example"
  },
  "author": "Sid is Live YT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SidisLiveYT/node-vimeo.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SidisLiveYT/node-vimeo/issues"
  },
  "homepage": "https://node-vimeo.js.org",
  "keywords": [
    "discord",
    "node-vimeo",
    "vimeo",
    "vimeo-api",
    "framework",
    "vimeo-parser",
    "vimeo-scrapper"
  ]
}
